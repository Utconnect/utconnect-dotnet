@{
  if (ViewData.TryGetValue("ParentLayout", out object? parentLayout) && parentLayout != null)
  {
    Layout = parentLayout.ToString();
  }
}

<h1>Quản lý tài khoản</h1>

<div>
  <hr/>
  <div class="row">
    <div class="col-md-3">
      <partial name="_ManageNav"/>
    </div>
    <div class="col-md-9">
      @RenderBody()
    </div>
  </div>
</div>

@section Scripts {
  @await RenderSectionAsync("Scripts", required: false)
}