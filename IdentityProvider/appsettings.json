{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityProviderContextConnection": "Host=identity_provider_db;Port=5432;Username=postgres;Password=example;Database=Identity"
  },
  "Coffer": "http://coffer_api:8080",
  "HomeConfig": {
    "Url": "http://localhost:5000",
    "TokenPath": "/token",
    "LogoutPath": "/logout"
  },
  "OidcConfig": {
    "Url": "http://oidc:8080",
    "TokenPath": "/token",
    "Jwt": {
      "Issuer": "Utconnect.IdentityProvider",
      "Audience": "Utconnect.Oidc",
      "ExpirationMinutes": 1
    }
  }
}